<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
    <context:annotation-config />

  <bean class="com.rohit.springcore.autowiring.Address" name="address1">
     <property name="city" value="Mohali"></property>
     <property name="state" value="Punjab"></property>
     
  </bean>
  
  <bean class="com.rohit.springcore.autowiring.Address" name="address2">
     <property name="city" value="Ludhiana"></property>
     <property name="state" value="Punjab"></property>
     
  </bean>
  
  <bean class="com.rohit.springcore.autowiring.Employee" name="emp">
    <property name="empId" value="3535"></property>
     
  </bean>

</beans>